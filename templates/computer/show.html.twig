{% extends 'base.html.twig' %}

{% block title %}Computer{% endblock %}

{% block body %}
    <h1>Computer</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ computer.id }}</td>
            </tr>
            <tr>
                <th>Modele</th>
                <td>{{ computer.modele }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ computer.description }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ computer.prix }}</td>
            </tr>
            <tr>
                <th>Annee_sortie</th>
                <td>{{ computer.anneeSortie }}</td>
            </tr>
            <tr>
                <th>Numero_serie</th>
                <td>{{ computer.numeroSerie }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ computer.image }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_computer_index') }}">back to list</a>

    <a href="{{ path('app_computer_edit', {'id': computer.id}) }}">edit</a>

    {{ include('computer/_delete_form.html.twig') }}
{% endblock %}
